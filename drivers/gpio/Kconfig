config GPIOLIB
	bool
	select GENERIC_GPIO

if GPIOLIB

menu "GPIO                          "

config GPIO_PL061
	bool "PrimeCell PL061 GPIO support"
	depends on ARM_AMBA
	help
	  Say yes here to support the PrimeCell PL061 GPIO device

config GPIO_STMPE
	depends on I2C_STMPE
	bool "STMPE GPIO Expander"
endmenu

endif
