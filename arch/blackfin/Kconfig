
config BLACKFIN
	bool
	default y

config BF561
	bool

config BOARDINFO
	default "PII IPE337" if MACH_IPE337

config ARCH_TEXT_BASE
	hex
	default 0x00000000

choice
	prompt "Select your board"

config MACH_IPE337
	bool "PII ipe337"
	select BF561
	help
	  Say Y here if you are using the PII IPE337 board

endchoice

